/// <summary>
/// Sets the color opaque.
/// </summary>
class ComputeColorOpaque : ComputeColor
{
    compose ComputeColor color1;

    override float4 Compute()
    {
        float4 computedColor = color1.Compute();
        computedColor.w = 1.0;
        return computedColor;
    }
};